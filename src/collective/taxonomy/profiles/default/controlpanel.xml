<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.taxonomy"
    purge="False">

    <configlet
        title="Taxonomies"
        action_id="taxonomies"
        appId="collective.taxonomy"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@taxonomy-settings"
        icon_expr="string:$portal_url/product_icon.png"
        visible="True"
        i18n:attributes="title">
      <permission>Manage taxonomies</permission>
    </configlet>

</object>
